{
    "name": "yokessh/yoke",
    "description": "SSH Connection Manager and Keychain",
    "type": "project",
    "require": {
        "php": "^7.2",
        "symfony/console": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Yoke\\": "src/Yoke"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yoke\\Test\\": "tests/"
        }
    },
    "scripts": {
        "psr2": [
          "vendor/squizlabs/php_codesniffer/bin/phpcs.bat --standard=PSR2 --extensions=php,inc,lib src/"
        ]
    },
